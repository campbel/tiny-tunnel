    "version": "0.2.0",
    "compounds": [
        {
          "name": "All",
          "configurations": ["Server", "Client", "Echo"],
          "stopAll": true
        }
      ],
    "configurations": [
        {
            "name": "Server",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": ["server"],
        },
        {
            "name": "Echo",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": ["echo"],
        },
        {
            "name": "Client",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "client",
                "--name", "dev-tunnel",
                "http://127.0.0.1:7000"
            ],
        },
        {
            "name": "errrrr",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "client",
                "tootoooo",
                "qweqweqwe"
            ],
        }
    ]
}